let name ./N30/N30-CH3OH-8--1-7-0-selfcal
let map_cell 0.03
let map_size 800
let map_robust 3.0
let weight_mode robust
input uvmap

go uvmap

let niter 600
let fres 0.01
input clean

go clean

!! # export .fits file
vector\fits ./N30/N30-CH3OH-8--1-7-0-selfcal.fits from ./N30/N30-CH3OH-8--1-7-0-selfcal.lmv-clean /overwrite
